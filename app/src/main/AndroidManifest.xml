<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.randy.to_be_determined">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/spot_swap_launcher"
        android:label="SpotSwap"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateAccountActivity" />
        <activity android:name=".MainActivity">
            android:parentActivityName="com.example.randy.to_be_determined.LogInActivity"
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.randy.to_be_determined.LogInActivity" />
        </activity>
        <activity
            android:name=".ListOfSpots"
            android:label="@string/title_activity_list_of_spots"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
